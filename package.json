{
    "name": "fractal-generator",
    "version": "1.0.0",
    "description": "Fractal images generator using canvas",
    "main": "index.js",
    "scripts": {
        "build": "node sass.js",
        "docker:build": "docker build -t fractal-generator .",
        "docker:run": "docker run --name fractal-generator -p 80:3000 -d fractal-generator",
        "docker:stop": "docker stop fractal-generator && docker rm fractal-generator",
        "start": "node index.js"
    },
    "author": "Carles Capellas <capellas.carles@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "express": "4.17.1",
        "node-sass": "7.0.0"
    }
}
